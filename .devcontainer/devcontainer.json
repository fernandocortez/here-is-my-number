{
	"name": "here-is-my-number [Sandbox]",
	"image": "mcr.microsoft.com/devcontainers/javascript-node:24-trixie",
	"features": {
		"ghcr.io/devcontainers/features/common-utils:2": {
			"configureZshAsDefaultShell": true
		}
	},
	"forwardPorts": [5173],
	"postCreateCommand": "echo alias pn=pnpm >> ~/.zshrc",
	"customizations": {
		"vscode": {
			"extensions": ["dbaeumer.vscode-eslint", "prettier.prettier-vscode"]
		}
	}
}
